{
  "name": "leowebguy/mix-manifest",
  "description": "A minimal Craft 3 plugin to load assets from mix-manifest.json",
  "version": "1.0.0",
  "type": "craft-plugin",
  "license": "MIT",
  "authors": [
    {
      "name": "Leo",
      "homepage": "https://github.com/leowebguy"
    }
  ],
  "require": {
    "craftcms/cms": "^3.4.0-RC1"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.18"
  },
  "autoload": {
    "psr-4": {
      "leowebguy\\mixmanifest\\": "src/"
    }
  },
  "extra": {
    "name": "Mix Manifest",
    "handle": "mix-manifest",
    "hasCpSection": false,
    "hasCpSettings": false,
    "components": {
      "mixService": "leowebguy\\mixmanifest\\services\\MixManifestService"
    },
    "class": "leowebguy\\mixmanifest\\MixManifest"
  },
  "scripts": {
    "lint": "vendor/bin/php-cs-fixer fix src --dry-run --rules=@PSR2 -vvv",
    "lint-fix": "vendor/bin/php-cs-fixer fix src --rules=@PSR2 -vvv"
  }
}
